{
  "name": "ip2location-lookup",
  "displayName": "IP2Location Lookup",
  "description": "Look up IP location information using IP2Location.IO",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:ip2location.lookup",
    "onCommand:ip2location.lookupSelection",
    "onCommand:ip2location.setApiKey"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ip2location.lookup",
        "title": "IP2Location: Lookup IP Address"
      },
      {
        "command": "ip2location.lookupSelection",
        "title": "IP2Location: Lookup Selected IP"
      },
      {
        "command": "ip2location.setApiKey",
        "title": "IP2Location: Set API Key"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "ip2location.lookupSelection",
          "group": "ip2location"
        }
      ]
    },
    "configuration": {
      "title": "IP2Location",
      "properties": {
        "ip2location.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your IP2Location.IO API key"
        },
        "ip2location.defaultFields": {
          "type": "array",
          "default": [
            "country_code",
            "country_name",
            "region_name",
            "city_name",
            "latitude",
            "longitude",
            "zip_code",
            "time_zone",
            "asn",
            "as",
            "is_proxy"
          ],
          "description": "Fields to display in results"
        }
      }
    },
    "keybindings": [
      {
        "command": "ip2location.lookupSelection",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l",
        "when": "editorTextFocus && editorHasSelection"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
